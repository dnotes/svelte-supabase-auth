{
	"name": "test-svelte",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev --port=8081",
		"build": "vite build",
		"preview": "vite preview --port=8081",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test": "vitest --run",
		"test:watch": "vitest",
		"playwright-install-ci": "playwright install --with-deps"
	},
	"devDependencies": {
		"@quickpickle/playwright": "^1.0",
		"@supabase/supabase-js": "^2.57.3",
		"@sveltejs/adapter-auto": "^6.0.0",
		"@sveltejs/kit": "^2.22.0",
		"@sveltejs/vite-plugin-svelte": "^6.0.0",
		"playwright": "^1.55.0",
		"quickpickle": "^1.10.1",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"typescript": "^5.0.0",
		"vite": "^7.0.4",
		"vitest": "^3.2.4"
	},
	"dependencies": {
		"svelte-supabase-auth": "workspace:^"
	}
}
