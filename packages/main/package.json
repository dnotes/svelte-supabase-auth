{
  "name": "svelte-supabase-auth",
  "version": "0.4.0",
  "description": "A comprehensive, configurable, Svelte-based authentication UI component library for Supabase applications.",
  "author": "David Hunt",
  "license": "ISC",
  "homepage": "https://svelte-supabase-auth.pages.dev",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dnotes/svelte-supabase-auth"
  },
  "bugs": {
    "url": "https://github.com/dnotes/svelte-supabase-auth/issues"
  },
  "scripts": {
    "dev": "vite dev --port 5173",
    "build": "vite build && sleep 2s && pnpm prepack",
    "preview": "vite preview --port 5173",
    "prepare": "svelte-kit sync || echo ''",
    "prepack": "svelte-kit sync && svelte-package && vite build --config vite.web-component.config.ts && cp ./static/webcomponent/*.* ./dist/ && cp ./node_modules/@supabase/supabase-js/dist/umd/supabase.js ./static/webcomponent && publint",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test": "rm -r errors; vitest run",
    "test:watch": "rm -r errors; vitest",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "playwright-install-ci": "playwright install --with-deps"
  },
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "sideEffects": [
    "dist/i18n/languages/*.js",
    "**/*.css"
  ],
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    },
    "./web-component": {
      "types": "./dist/web-component.d.ts",
      "import": "./dist/supabase-auth-web-component.js",
      "require": "./dist/supabase-auth-web-component.umd.cjs"
    },
    "./supabase-auth-web-component.js": "./dist/supabase-auth-web-component.js",
    "./supabase-auth-web-component.umd.cjs": "./dist/supabase-auth-web-component.umd.cjs"
  },
  "dependencies": {
    "hibp": "^15.0.1"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.7",
    "@chromatic-com/storybook": "^4.1.1",
    "@iconify/json": "^2.2.382",
    "@quickpickle/playwright": "^1.0",
    "@storybook/addon-a11y": "^9.1.5",
    "@storybook/addon-docs": "^9.1.5",
    "@storybook/addon-svelte-csf": "^5.0.8",
    "@storybook/addon-vitest": "^9.1.5",
    "@storybook/sveltekit": "^9.1.5",
    "@supabase/supabase-js": "^2.57.3",
    "@sveltejs/adapter-cloudflare": "^7.2.3",
    "@sveltejs/adapter-static": "^3.0.9",
    "@sveltejs/kit": "^2.37.1",
    "@sveltejs/package": "^2.5.0",
    "@sveltejs/vite-plugin-svelte": "^5.1.1",
    "@tailwindcss/typography": "^0.5.15",
    "@tailwindcss/vite": "^4.0.0",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.19.13",
    "@types/speakeasy": "^2.0.10",
    "iso-639-1": "^3.1.5",
    "lodash-es": "^4.17.21",
    "mdsvex": "^0.12.6",
    "playwright": "^1.55.0",
    "publint": "^0.3.12",
    "quickpickle": "^1.10.1",
    "speakeasy": "^2.0.0",
    "storybook": "^9.1.5",
    "svelte": "^5.38.7",
    "svelte-check": "^4.3.1",
    "tailwindcss": "^4.0.0",
    "typescript": "^5.9.2",
    "unplugin-icons": "^22.4.2",
    "vite": "^6.3.6",
    "vite-plugin-toml": "^0.8.4",
    "vitest": "^3.2.4"
  },
  "keywords": [
    "svelte",
    "supabase",
    "oauth",
    "authentication"
  ],
  "peerDependencies": {
    "@supabase/supabase-js": "^2.0.0",
    "svelte": "^5.0.0"
  }
}